    "wordlist = ['one', 'two', 'three', 'six']\n",
    "\n",
    "d = {}\n",
    "\n",
    "for word in wordlist:\n",
    "    myLen = len(word)\n",
    "    \n",
    "    if myLen in d:\n",
    "        d[myLen].append(word)\n",
    "        d[myLen].sort()\n",
    "        \n",
    "    else:\n",
    "        d[myLen] = [word]\n",
    "            \n",
    "print(d)"
